    "lint-bench": "jshint --verbose --reporter node_modules/jshint-stylish/stylish.js ./benchmark/*.js",
    "lint-lib": "jshint --verbose --reporter node_modules/jshint-stylish/stylish.js *.js",
    "lint-test": "jshint --verbose --reporter node_modules/jshint-stylish/stylish.js ./spec/*.js",
    "test": "jasmine-node spec --verbose"
  },
  "keywords": [
